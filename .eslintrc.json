{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
        // "arrowFunctions": true,
        // "classes": true
    },
    "rules": {
        "max-classes-per-file": 0,
        "import/extensions": 0,
        "object-curly-newline": 0,
        "no-alert": 0,
        "no-return-assign": 0,
        "no-param-reassign": 0

    }
}
